<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.studyfromvideo"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />
    
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.studyfromvideo.MainActivity"
            android:label="@string/title_activity_main" >
            <intent-filter >
                <action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />               
            </intent-filter>
        </activity>
        <activity android:name="com.example.studyfromvideo.IntentTestActivity" 
            android:label="@string/title_activity_intentTest">
        </activity>
         <activity android:name="com.example.studyfromvideo.HandlerTestActivity"
                   android:label="@string/title_activity_handlerTest">
        </activity>
        <activity android:name="com.example.studyfromvideo.SQLiteTestActivity"
                  android:label="@string/title_activity_sqliteTest">
        </activity>
           <activity android:name="com.example.studyfromvideo.DownloadTestActivity"
                  android:label="@string/title_activity_downloadTest">
        </activity>
    </application>

</manifest>
