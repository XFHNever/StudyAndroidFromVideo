<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.studyfromvideo"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />
    
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.studyfromvideo.MainActivity"
            android:label="@string/title_activity_main" >
            <intent-filter >
                <action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />               
            </intent-filter>
        </activity>
        <activity android:name="com.example.studyfromvideo.intent.IntentTestActivity" 
            android:label="@string/title_activity_intentTest">
        </activity>
         <activity android:name="com.example.studyfromvideo.handler.HandlerTestActivity"
                   android:label="@string/title_activity_handlerTest">
        </activity>
        <activity android:name="com.example.studyfromvideo.sqlite.SQLiteTestActivity"
                  android:label="@string/title_activity_sqliteTest">
        </activity>
           <activity android:name="com.example.studyfromvideo.download.DownloadTestActivity"
                  android:label="@string/title_activity_downloadTest">
        </activity>
        <activity android:name="com.example.studyfromvideo.broadcast.BroadcastTestActivity"
                  android:label="@string/title_activity_broadcastTest">
        </activity>
        <activity android:name="com.example.studyfromvideo.xml.SAXTestActivity"
                  android:label="@string/title_activity_saxTest">
        </activity>
         <activity android:name="com.example.studyfromvideo.contentprovider.CPTestActivity"
                  android:label="@string/title_activity_cpTest">
        </activity>
        
        
        
        <receiver android:name="com.example.studyfromvideo.broadcast.MyReceiver1">
            <intent-filter >
                <action android:name="android.intent.action.MEDIA_BUTTON"/>
            </intent-filter>
        </receiver>
        
        <provider android:name="com.example.studyfromvideo.contentprovider.FirstContentProvider"
            android:authorities="com.example.studyfromvideo.contentprovider.FirstContentProvider"/>
    </application>

</manifest>
